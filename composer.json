{
    "name": "teganbold/markdownconverter",
    "type": "project",
    "description": "Command line applet to convert Markdown syntax to HTML markup.",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.75"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
